@page "/contact"

<PageTitle>Redes Sociais - Gabriel Santos</PageTitle>

<main class="contact-page">
    <section class="header-info">
        <img src="https://placehold.co/150x150" alt="Imagem de perfil" width="150" height="150" />
        <h3>&commat;aquelegabes</h3>
        <p>Consectetur elit quis nisi occaecat in tempor do nulla exercitation commodo veniam.</p>
    </section>
    <nav class="social-media">
        <ul class="links">
            @foreach (var info in infos)
            {
                <li>
                    <a href="@(info.Url)">
                        <span class="ico @(info.CSSIcon)">
                        </span>
                        <span class="social-media-name">@(info.Name)</span>
                    </a>
                </li>
            }
        </ul>
    </nav>
</main>